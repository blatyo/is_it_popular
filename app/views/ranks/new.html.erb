<h1>is <span id="site">it</span> popular<span id="question">?</span></h1>

<%= form_for @page_rank, :url => ranks_path do |f| %>
  <p id="label"><%= f.label :site, "Enter a site" %></p>
  <p id="input">
    <%= f.text_field :site %>
    <%= f.submit "Go" %>
  </p>
  <p id="example">
    e.g. www.google.com
  </p>
<% end %>
